VAR
  numprojctrl    IN SHADOW FROM GMCFGPJW: INTEGER                 --Number of projection weld controllers
  projcommtype   IN SHADOW FROM GMCFGPJW: INTEGER                 --Project Weld communication type
  projwldapp    IN SHADOW FROM GMCFGPJW: ARRAY[2] OF GM_PROJCG   --GM projection weld equipment configuration
